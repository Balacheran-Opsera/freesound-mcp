{% import './atoms/icons.njk' as icons %}
{% import './molecules/navbar.njk' as navbar %}
{% import './pages/followers/follows.njk' as follows %}
{% extends "base.njk" %}

{% block content %}
  <div class="container bw-followers__main">
    <div class="bw-followers__close v-spacing-top-4">
      <a href="">{{ icons.icon('close') }}</a>
    </div>
    {{ navbar.followersNavbar('followers') }}
    <div class="col-8 offset-2 v-spacing-top-6">
      {{ follows.results(10) }}
    </div>
  </div>
{% endblock %}